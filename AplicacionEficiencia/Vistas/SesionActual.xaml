        <Label x:FieldModifier="public" x:Name="testSesion" Content="Label" HorizontalAlignment="Left" Margin="34,179,0,0" VerticalAlignment="Top"  Width="692"/>
        <Button x:Name="button" Content="Finalizar Sesion" HorizontalAlignment="Left" Margin="48,397,0,0" VerticalAlignment="Top" Width="127" Click="button_Click"/>

    </Grid>
