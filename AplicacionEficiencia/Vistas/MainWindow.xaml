<Window x:Class="AplicacionEficiencia.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AplicacionEficiencia"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FF717070">
        <StackPanel Margin="0,0,542,0" HorizontalAlignment="Left">
            <Button x:Name="button" Content="ModificarPerfil" Click="button_Click" Width="188" Height="20"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Margin="188,0,0,0" Background="Red">
            <Frame x:Name="frame" Content="" Background="Red"/>
        </StackPanel>
    </Grid>
</Window>
